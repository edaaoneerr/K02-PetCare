<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/onboardingFragment">
    <fragment
        android:id="@+id/getStartedFragment"
        android:name="com.example.petcareproject.views.greetingview.GetStartedFragment"
        android:label="GetStartedFragment">
        <action
            android:id="@+id/action_getStartedFragment_to_getStartedFragment2"
            app:destination="@id/getStartedFragment2" />
    </fragment>
    <fragment
        android:id="@+id/getStartedFragment2"
        android:name="com.example.petcareproject.views.greetingview.GetStartedFragment2"
        android:label="fragment_get_started2"
        tools:layout="@layout/fragment_get_started2" >
        <action
            android:id="@+id/action_getStartedFragment2_to_getStartedFragment3"
            app:destination="@id/getStartedFragment3" />
    </fragment>
    <fragment
        android:id="@+id/getStartedFragment3"
        android:name="com.example.petcareproject.views.greetingview.GetStartedFragment3"
        android:label="fragment_get_started3"
        tools:layout="@layout/fragment_get_started3" >
    </fragment>

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.petcareproject.views.authview.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/onboardingFragment"
        android:name="com.example.petcareproject.views.greetingview.OnboardingFragment"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding" >
        <action
            android:id="@+id/action_onboardingFragment_to_getStartedFragment"
            app:destination="@id/getStartedFragment" />
        <action
            android:id="@+id/action_onboardingFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.petcareproject.views.authview.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_registerFragment_to_vetRegisterFragment"
            app:destination="@id/vetRegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.petcareproject.views.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_vetClinicListFragment"
            app:destination="@id/vetClinicListFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_homeFragment_to_vetListFragment"
            app:destination="@id/vetListFragment" />
        <action
            android:id="@+id/action_homeFragment_to_vetDetailFragment"
            app:destination="@id/vetDetailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bookingsFragment"
            app:destination="@id/bookingsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_petFragment"
            app:destination="@id/petFragment" />
    </fragment>
    <fragment
        android:id="@+id/vetRegisterFragment"
        android:name="com.example.petcareproject.views.authview.VetRegisterFragment"
        android:label="fragment_vet_register"
        tools:layout="@layout/fragment_vet_register" >
        <action
            android:id="@+id/action_vetRegisterFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <argument
            android:name="vetUser"
            app:argType="com.example.petcareproject.model.User" />
    </fragment>
    <fragment
        android:id="@+id/vetClinicListFragment"
        android:name="com.example.petcareproject.views.vetview.VetClinicListFragment"
        android:label="VetClinicListFragment" >
        <action
            android:id="@+id/action_vetClinicListFragment_to_vetDetailFragment"
            app:destination="@id/vetDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/vetDetailFragment"
        android:name="com.example.petcareproject.views.vetview.VetDetailFragment"
        android:label="fragment_vet_detail"
        tools:layout="@layout/fragment_vet_detail" >
        <action
            android:id="@+id/action_vetDetailFragment_to_appointmentFragment"
            app:destination="@id/appointmentFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.petcareproject.views.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.petcareproject.views.messageview.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" >
        <action
            android:id="@+id/action_chatFragment_to_messageFragment"
            app:destination="@id/messageFragment" />
    </fragment>
    <fragment
        android:id="@+id/messageFragment"
        android:name="com.example.petcareproject.views.messageview.MessageFragment"
        android:label="fragment_message"
        tools:layout="@layout/fragment_message" >
    </fragment>
    <fragment
        android:id="@+id/vetListFragment"
        android:name="com.example.petcareproject.views.vetview.VetListFragment"
        android:label="fragment_vet_list"
        tools:layout="@layout/fragment_vet_list" >
        <action
            android:id="@+id/action_vetListFragment_to_vetDetailFragment"
            app:destination="@id/vetDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/appointmentFragment"
        android:name="com.example.petcareproject.views.appointmentview.AppointmentFragment"
        android:label="AppointmentFragment" />
    <fragment
        android:id="@+id/bookingsFragment"
        android:name="com.example.petcareproject.views.appointmentview.BookingFragment"
        android:label="BookingsFragment" />
    <fragment
        android:id="@+id/petFragment"
        android:name="com.example.petcareproject.views.petview.PetFragment"
        android:label="PetFragment" >
        <action
            android:id="@+id/action_petFragment_to_petHealthFragment"
            app:destination="@id/petHealthFragment" />
        <action
            android:id="@+id/action_petFragment_to_petNutritionFragment"
            app:destination="@id/petNutritionFragment" />
        <action
            android:id="@+id/action_petFragment_to_petActivityFragment"
            app:destination="@id/petActivityFragment" />
    </fragment>
    <fragment
        android:id="@+id/petHealthFragment"
        android:name="com.example.petcareproject.views.petview.PetHealthFragment"
        android:label="fragment_pet_health"
        tools:layout="@layout/fragment_pet_health" >
        <action
            android:id="@+id/action_petHealthFragment_to_petInsuranceFragment"
            app:destination="@id/petInsuranceFragment" />
    </fragment>
    <fragment
        android:id="@+id/petNutritionFragment"
        android:name="com.example.petcareproject.views.petview.PetNutritionFragment"
        android:label="fragment_pet_nutrition"
        tools:layout="@layout/fragment_pet_nutrition" />
    <fragment
        android:id="@+id/petActivityFragment"
        android:name="com.example.petcareproject.views.petview.PetActivityFragment"
        android:label="fragment_pet_activity"
        tools:layout="@layout/fragment_pet_activity" />
    <fragment
        android:id="@+id/petInsuranceFragment"
        android:name="com.example.petcareproject.views.petview.PetInsuranceFragment"
        android:label="PetInsuranceFragment" >
        <action
            android:id="@+id/action_petInsuranceFragment_to_petInsurancePackagesFragment"
            app:destination="@id/petInsurancePackagesFragment" />
    </fragment>
    <fragment
        android:id="@+id/petInsurancePackagesFragment"
        android:name="com.example.petcareproject.views.petview.PetInsurancePackagesFragment"
        android:label="PetInsurancePackagesFragment" />
</navigation>